{
  "name": "railroad/customer-io",
  "type": "library",
  "description": "Integration with customer.io crm and tracking which customers are associated with which users.",
  "keywords": [
    "customer.io",
    "crm",
    "lead"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\CustomerIo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\CustomerIo\\Tests\\": "tests/"
    }
  }
}
