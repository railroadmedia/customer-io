{
  "name": "railroad/customer-io",
  "type": "library",
  "description": "Integration with customer.io crm and tracking which customers are associated with which users.",
  "keywords": [
    "customer.io",
    "crm",
    "lead"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "~7",
    "laravel/framework": "~5.6",
    "mockery/mockery": "^1.4",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "orchestra/database": "~3.0",
    "orchestra/testbench": "~3.0",
    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\CustomerIo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\CustomerIo\\Tests\\": "tests/"
    }
  }
}
